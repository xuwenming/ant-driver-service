page {
  background-color: #f6f6f6;
  height: 100%;
}
.container {
  /* display: flex; */
  flex-direction: column;
  min-height: 100%;
  justify-content: space-between;
  overflow-x: hidden;
}

 button[type="blue"][plain] {
  color:#05C0AB;
  border:1px solid #05C0AB;
  background-color:transparent;
}

button[type="blue"][disabled] {
  color: #D9D9D9; 
  background-color:transparent;
  border: 0;
}

button[type="blue"]{
  border:1rpx solid #05C0AB;
  background-color:#05C0AB;
  color: #fff;
} 

button[type="yellow"][plain] {
  color:#fe9900;
  border:1px solid #fe9900;
  background-color:transparent;
}

button[type="yellow"][disabled] {
  color: #D9D9D9; 
  background-color:transparent;
  border: 0;
}

button[type="yellow"]{
  border:1rpx solid #fe9900;
  background-color:#fe9900;
  color: #fff;
} 
